{
  "name": "nextjs-hybrid-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:api": "cd api && npm run build",
    "swa:all": "swa start",
    "swa:build": "npm run build && (cd api && npm run build)",
    "swa:start": "swa start out --api-location api",
    "test:e2e": "WITH_API=true npx playwright test",
    "test:e2e:headed": "WITH_API=true npx playwright test --headed",
    "test:e2e:mock": "npx playwright test",
    "test:e2e:mock:headed": "npx playwright test --headed",
    "test:e2e:swa": "npx playwright test --config=playwright.config.swa.ts",
    "test:e2e:swa:headed": "npx playwright test --config=playwright.config.swa.ts --headed",
    "dev:with-api": "npm run build:api && swa start http://localhost:3000 --api-location api --run \"npm run dev\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "canvas": "^3.1.0",
    "highlight.js": "^11.11.1",
    "next": "14.2.30",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "engines": {
    "node": "18.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaotiantakumi/SwaSnapContent.git"
  }
}
